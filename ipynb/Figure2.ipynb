    "kname = 'f58ef02fd3fe4f8f1c8ba13a3e205697'\n",
    "def alr(x):\n",
    "    return x / x.loc[kname]\n",
    "alr_raw = raw_df.apply(alr, axis=1)\n",
    "alr_cell = cell_df.apply(alr, axis=1)"
